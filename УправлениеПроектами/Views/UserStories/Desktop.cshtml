@model МенеджерБД.Домен.Требование

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Страница требования: @Model.Название</title>

    <!--подключение ExtJS------------------------------------------>
    <link href="/Scripts/ExtJS/resources/css/ext-neptune.css" rel="stylesheet">
    <script src="/Scripts/ExtJS/ext-all-debug.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/locale/ext-lang-ru.js" type="text/javascript"></script>
</head>
<body>
    <div>
        <div class="container">
            <div id="desktop">
            </div>
        </div>


        <script type="text/javascript">
            var userStoryId = @Model.Id;
            var defaultValues = {
                projectId: @Model.Проект.Id,
                categoryId: @Model.Категория.Id,
                name: "@Model.Название",
                description: "@Model.Описание",
                estimate: @Model.Оценка,
                importance: @Model.Важность
            };
        </script>
        <script src="/Scripts/CustomScripts/main-menu-panel.js" type="text/javascript"></script>
        <script src="/Scripts/CustomScripts/win-create-user-story.js" type="text/javascript"></script>
        <script src="/Scripts/CustomScripts/user-story-desktop.js" type="text/javascript"></script>
    </div>
</body>
</html>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!--Styles.Render("~/Content/Style.css")-->
    <!--Styles.Render("~/Content/themes/base/css", "~/Content/css")-->
    <!--script src="//ui/1.10.4/jquery-ui.js"></!--script>-->
    <!--подключение ExtJS------------------------------------------>
    @Styles.Render("~/Content/css/ExtJS")
    @Scripts.Render("~/bundles/ExtJS")
    <!--основной скриптовый файл приложения------------------------>
    @Scripts.Render("~/bundles/app")
    <!--стили приложения------------------------------------------->
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/bootstrap")
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/modernizr")
    @*<script>
        $(function () {
            $("#datepicker").datepicker();
        });
    </script>*@
</head>

<body>
<!--Панель навигации-------------------------------------------->
     <div class="navbar navbar-fixed-top" style="width:1000px;margin:0 auto">
         <div class="navbar-inner">
             <div class="container">
                 <a class="brand">Система управления проектами</a>
                 <ul class="nav nav-pills pull-right">
                     <li><span class="btn btn-link">@Html.ActionLink("Проекты", "Index", "Projects")</span></li>
                     <li><span class="btn btn-link">@Html.ActionLink("Спринты", "Index", "Sprints")</span></li>
                     <li><span class="btn btn-link">@Html.ActionLink("Требования", "Index", "UserStories")</span></li>
                     <li><span class="btn btn-link">@Html.ActionLink("Задачи", "Index", "Tasks")</span></li>
                     <li><span class="btn btn-link">|</span></li>
                     @Html.Action("UserLogin", "Home")
                 </ul>                                   
             </div>
         </div>
     </div>
<!---------------------------------------------------------------->
 
    @RenderBody()
    <div id="PopupWrapper"></div>
    @Scripts.Render("~/bundles/jquerydev")
    @Scripts.Render("~/bundles/jquerydevui")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquerydevuidate")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/common")
    @Styles.Render("~/Content/css/jquerydevui")




    @RenderSection("scripts", required: false)
</body>

</html>

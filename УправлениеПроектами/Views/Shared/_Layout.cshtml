<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!--подключение ExtJS------------------------------------------>
    @Styles.Render("~/Content/css/ExtJS")
    @Scripts.Render("~/bundles/ExtJS")
    <!--основной скриптовый файл приложения------------------------>
    @Scripts.Render("~/bundles/app")
    <!--стили приложения------------------------------------------->
    @Styles.Render("~/Content/css")
  @*  @Styles.Render("~/Content/bootstrap")*@
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
<!--Панель навигации-------------------------------------------->
     <div class="navbar navbar-fixed-top" style="width:96%;margin:0 auto">
         <div class="navbar-inner">
             <div class="container">
                 <a class="brand">Система управления проектами</a>
                 <ul class="nav nav-pills pull-right">
                     @Html.Action("Menu", "Home")
                     <li><span class="btn btn-link">|</span></li>
                     @Html.Action("UserLogin", "Home")
                 </ul>                                   
             </div>
         </div>
     </div>
<!---------------------------------------------------------------->
 
    @RenderBody()
    <div id="PopupWrapper"></div>
    @Scripts.Render("~/bundles/jquerydev")
    @Scripts.Render("~/bundles/jquerydevui")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquerydevuidate")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/common")
    @Styles.Render("~/Content/css/jquerydevui")




    @RenderSection("scripts", required: false)
</body>

</html>

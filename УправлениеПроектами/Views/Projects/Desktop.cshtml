    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@string.Format("Страница проекта \"{0}\"", Model.Название)</title>

    <!--подключение ExtJS------------------------------------------>
    <link href="/Scripts/ExtJS/resources/css/ext-neptune.css" rel="stylesheet">
    <script src="/Scripts/ExtJS/ext-all-debug.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/locale/ext-lang-ru.js" type="text/javascript"></script>
    <!--подключение файлов необходимых для фильтарции в гриде-->
    <link href="/Scripts/ExtJS/examples/ux/grid/css/GridFilters.css" rel="stylesheet">
    <link href="/Scripts/ExtJS/examples/ux/grid/css/RangeMenu.css" rel="stylesheet">
    <script src="/Scripts/ExtJS/examples/ux/grid/FiltersFeature.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/TransformGrid.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/menu/ListMenu.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/menu/RangeMenu.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/BooleanFilter.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/DateFilter.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/Filter.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/ListFilter.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/NumericFilter.js" type="text/javascript"></script>
    <script src="/Scripts/ExtJS/examples/ux/grid/filter/StringFilter.js" type="text/javascript"></script>
</head>
<body>
    <div class="container">
        <div id="desktop" class="hero-unit" style="margin-top:45px; padding: 0px">
        </div>
    </div>


    <script type="text/javascript">var projectId = @Model.Id;</script>
    <script src="/Scripts/CustomScripts/grid-backlog.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/grid-sprints.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/grid-user-stories.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/win-create-user-story.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/win-create-sprint.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/win-sprint.js" type="text/javascript"></script>
    <script src="/Scripts/CustomScripts/project-desktop.js" type="text/javascript"></script>

<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="МенеджерБД" namespace="МенеджерБД.Домен">
  
  <class name="Задачи" table="pm.Tasks">
    <id name="Id"  type="Int32" unsaved-value="0" generator="increment" />
    <property name="IdТребования" column="user_story_id" />
    <property name="IdСпринта" column="Sprint_id" />
    <property name="IdАвтора" column="Author_id" />
    <property name="IdИсполнителя" column="current_executor_id" />
    <property name="Название" column="Name" />
    <property name="Описание" column="Description" />
  </class>

</hibernate-mapping>